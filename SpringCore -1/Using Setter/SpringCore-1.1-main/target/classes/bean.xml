<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd 
    http://www.springframework.org/schema/context/ 
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <beans>
    <bean id="Customer" class="Customer">
      <property name="customerId" value="1"></property>
    <property name="customerName" value="sk"></property>
        <property name="customerContact" value="6374879236"></property>
        <property name="customerAddress" ref="Address"></property>
    
    </bean>
    
    
    
    <bean id="Address" class="Address">
      <property name="street" value="NGGO Colony"></property>
    <property name="city" value="RKV road"></property>
        <property name="zip" value="638005"></property>
        <property name="country" value="India"></property>
    
    </bean>
    
    
    
    </beans>
    
    </beans>